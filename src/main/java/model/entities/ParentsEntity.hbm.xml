<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.entities.ParentsEntity" table="parents" schema="programming_tutorial">
        <id name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </id>
        <property name="child">
            <column name="child" sql-type="int(11)"/>
        </property>
        <property name="parentscol">
            <column name="parentscol" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>